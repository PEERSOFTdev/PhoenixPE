[Main]
Title=Host Boot
Description=Configure your PC to boot PhoenixPE directly from your hard-drive.
Author=Homes32
Level=9
Selected=None
Mandatory=False
Version=1.2.0.0
Date=2021-05-06

[Variables]
%ProgramExe%="ConfigHostBoot.cmd"

[Process]
Echo,"Launching %ScriptTitle%..."

Run,%ScriptFile%,GenerateConfigHostBootCmd
ShellExecute,Hide,"%ProjectTemp%\%ProgramExe%","",%ProjectTemp%
Message,"The operation has been completed."

[#GenerateConfigHostBootCmd#]
// ===============================================================================================================================
// Name...........: GenerateConfigHostBootCmd
// Description....: 
// Syntax.........: Run,%ScriptFile%,GenerateConfigHostBootCmd
// Parameters.....: 
// Return values..: 
// Author.........: KYHI, Homes32
// Remarks........: 
// Related........: 
// ===============================================================================================================================
[GenerateConfigHostBootCmd]
Echo,"Creating Config Script..."

StrFormat,LTRIM,%OutputDir%,3,%drfldr%
StrFormat,LEFT,%OutputDir%,2,%drvlttr%

If,ExistFile,"%ProjectTemp%\%ProgramExe%",FileDelete,"%ProjectTemp%\%ProgramExe%"
FileCreateBlank,"%ProjectTemp%\%ProgramExe%"
TXTAddLine,"%ProjectTemp%\%ProgramExe%","",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","@echo Creating Host Boot Menu Option...",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","@cd /d #$q%~dp0#$q",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","@for /f #$qtokens=2 delims={}#$q %%g in ('bcdedit.exe /create /d #$q%txt_MenuName%#$q /Device') do (@set deviceid={%%g})",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","@echo Device ID: %deviceid%",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","bcdedit /set %deviceid% ramdisksdidevice partition=%drvlttr%",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","bcdedit /set %deviceid% ramdisksdipath \%drfldr%\Boot\boot.sdi",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","@for /f #$qtokens=2 delims={}#$q %%g in ('bcdedit.exe /create /d #$q%txt_MenuName%#$q /application osloader') do (@set guid={%%g})",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","@echo Menu GUID: %guid%",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","bcdedit /set %guid% device ramdisk=[%drvlttr%]\%drfldr%\sources\boot.wim,%deviceid%",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","bcdedit /set %guid% osdevice ramdisk=[%drvlttr%]\%drfldr%\sources\boot.wim,%deviceid%",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","bcdedit /set %guid% systemroot \windows",Append
If,%cmb_BIOSMode%,Equal,"Legacy",TXTAddLine,"%ProjectTemp%\%ProgramExe%","bcdedit /set %guid% path \Windows\system32\boot\winload.exe",Append
Else,If,%cmb_BIOSMode%,Equal,"UEFI",TXTAddLine,"%ProjectTemp%\%ProgramExe%","bcdedit /set %guid% path \Windows\system32\boot\winload.efi",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","bcdedit /set %guid% winpe yes",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","bcdedit /set %guid% detecthal yes",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","bcdedit /displayorder %guid% /addlast",Append
If,%cb_GraphicalBootMenu%,Equal,False,TXTAddLine,"%ProjectTemp%\%ProgramExe%","bcdedit /set {bootmgr} displaybootmenu yes",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","bcdedit /set {bootmgr} timeout %num_Timeout%",Append
TXTAddLine,"%ProjectTemp%\%ProgramExe%","@echo Done.",Append

[#RemoveBootMenuEntry#]
// ===============================================================================================================================
// Name...........: RemoveBootMenuEntry
// Description....: 
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_RemoveBootMenuEntry
// ===============================================================================================================================
[RemoveBootMenuEntry]
Message,"Removing an existing boot menu entry is a multi-step process.#$x#$x1. Click 'Ok' to display the System Configuration dialog.#$x2. Select the 'Boot' tab.#$x3. Select the PhoenixPE boot menu entry from the list.#$x4. Click the 'Delete' button.#$x5. Set Timeout to 0 seconds to prevent the boot menu from displaying on startup.#$x6. Click the 'OK' button.#$x7. If prompted to restart select 'Exit without restart'."
ShellExecuteEx,Open,"msconfig.exe"

[#ShowBIOSModeInfo#]
// ===============================================================================================================================
// Name...........: ShowBIOSModeInfo
// Description....: 
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_BIOSModeInfo
// ===============================================================================================================================
[ShowBIOSModeInfo]
Message,"When you click 'Ok' the System Information dialog will be displayed.#$x#$xYour BIOS Mode will be listed in the System Summary screen."
ShellExecuteEx,Open,"msinfo32.exe"

[#SetDefaultOptions#]
// ===============================================================================================================================
// Name...........: SetDefaultOptions
// Description....: Sets script options to default values.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: 
// Related........: btn_SetDefaultOptions
// ===============================================================================================================================
[SetDefaultOptions]
WriteInterface,Value,%ScriptFile%,Interface,cmb_BIOSMode,"Legacy"
WriteInterface,Value,%ScriptFile%,Interface,num_Timeout,10
WriteInterface,Value,%ScriptFile%,Interface,cb_GraphicalBootMenu,False
WriteInterface,Value,%ScriptFile%,Interface,txt_MenuName,"PhoenixPE"

