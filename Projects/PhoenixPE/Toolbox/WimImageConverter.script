[Main]
Title=Wim Image Converter
Description=Convert Electronic Software Distribution (.esd) images to standard .wim images and vice versa.
Author=Homes32
Level=10
Selected=None
Mandatory=False
Version=1.0.0.0
Date=2025-01-26

[Variables]

[Process]
Echo,"Converting image..."

// Sanity Check
If,Not,ExistFile,%fb_SrcPath%,Begin
  Message,"Error:#$x#$xYou must select a valid source image.#$x#$x%fb_SrcPath%#$x#$xdoes not exist!",Error
  Halt,"Error: Invalid source image. [%fb_SrcPath%] does not exist!"
End

StrFormat,Ext,%fb_SrcPath%,%SourceFileNameExt%
StrFormat,EndTrim,%fb_SrcPath%,%SourceFileNameExt%,%SourceFileNameNoExt%

If,%cmb_WimCompression%,Equal,"LZMS",Set,%FileExt%,".esd"
Else,Set,%FileExt%,".wim"

Echo,"Converting [%fb_SrcPath%] to [%SourceFileNameNoExt%%FileExt%]...#$x#$xThis can take awhile, please be patient."
WimOptimize,%fb_SrcPath%,ReComp=%cmb_WimCompression%

If,ExistFile,"%SourceFileNameNoExt%.wim",FileDeleteEx,"%SourceFileNameNoExt%%FileExt%"
FileRename,%fb_SrcPath%,"%SourceFileNameNoExt%%FileExt%"

