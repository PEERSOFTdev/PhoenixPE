[Main]
Title=Hasleo Backup Suite
Description=Hasleo Backup Suite Free is a free Windows backup, restore, and cloning software.
Author=Homes32
Level=5
Selected=False
Mandatory=False
Version=1.0.0.0
Date=2024-12-23

[Variables]
%ProgramFolder%="Hasleo Backup Suite"
%ProgramExe%="BackupMainUI.exe"
%ProgramVersion%=
%DownloadURL%=https://www.easyuefi.com/backup-software/downloads/Hasleo_Backup_Suite_Free.exe
%SetupFile%="Hasleo_Backup_Suite_Free.exe"
%ConfigFile%="%TargetPrograms%\%ProgramFolder%\bin\Backup Suite.ini"
%aLangMap%=Bulgarian|bg_BG|Chinese (Simplified)|zh_CN|Chinese (Traditional)|zh_TW|Dutch|nl_NL|English (US)|en_US|English (UK)|en_GB|French|fr_FR|German|de_DE|Hungarian|hu_HU|Italian|it_IT|Japanese|ja_JP|Korean|ko_KR|Polish|pl_PL|Portuguese (Brazil)|pt_BR|Russian|ru_RU|Slovak|sk_SK|Spanish|es_ES|Swedish|sv_SE|Turkish|tr_TR|Ukrainian|uk_UA

[Process]
Echo,"Processing %ScriptTitle%..."

If,%cb_RunFromRam%,Equal,True,RunFromRam

///////////////////////////////////////////////////////////////////////////////////
// Extract

If,%cb_AlwaysDownload%,Equal,True,DirDeleteEx,"%ProgramsCache%\%ProgramFolder%"
If,Not,ExistFile,"%ProgramsCache%\%ProgramFolder%\%SetupFile%",Run,%ScriptFile%,DownloadProgram
Run,%ScriptFile%,ExtractProgram,"%TargetPrograms%\%ProgramFolder%"

FileVersion,"%TargetPrograms%\%ProgramFolder%\bin\%ProgramExe%",%ProgramExeVer%
Echo,"Program version: v%ProgramExeVer%"

///////////////////////////////////////////////////////////////////////////////////
// Settings

Echo,"Configuring %ScriptTitle%..."

// Language
List,Pos,%aLangMap%,%cmb_Language%,%LangPos%
Math,Add,%Pos%,%LangPos%,1
List,Get,%aLangMap%,%Pos%,%Language%

IniWrite,%ConfigFile%,"Settings","CurrentLanguage",%Language%

// Disable updates
IniWrite,%ConfigFile%,"Settings","AutoCheckUpdate",0

RegLoadHives

// Register file extension
RegWrite,HKLM,REG_SZ,"Tmp_Software\Classes\.dbi","","Hasleo Backup Suite Image File"
RegWrite,HKLM,REG_SZ,"Tmp_Software\Classes\Hasleo Backup Suite Image File","","Hasleo Backup Suite Image File"
RegWrite,HKLM,REG_SZ,"Tmp_Software\Classes\Hasleo Backup Suite Image File\DefaultIcon","","%PEPrograms%\%ProgramFolder%\res\icon.ico"

// Error Reporting
RegWrite,HKLM,REG_DWORD,"Tmp_Software\Microsoft\Windows\Windows Error Reporting\LocalDumps\BackupService.exe","DumpType",1
RegWrite,HKLM,REG_SZ,"Tmp_Software\Microsoft\Windows\Windows Error Reporting\LocalDumps\BackupService.exe","DumpFolder","#$pTemp#$p\%ProgramFolder%"
RegWrite,HKLM,REG_DWORD,"Tmp_Software\Microsoft\Windows\Windows Error Reporting\LocalDumps\ImageMountService.exe","DumpType",1
RegWrite,HKLM,REG_EXPAND_SZ,"Tmp_Software\Microsoft\Windows\Windows Error Reporting\LocalDumps\ImageMountService.exe","DumpFolder","#$pTemp#$p\%ProgramFolder%"

// Backup Service
RegWrite,HKLM,REG_DWORD,"Tmp_System\ControlSet001\Services\HasleoBackupSuiteService","Type",16
RegWrite,HKLM,REG_DWORD,"Tmp_System\ControlSet001\Services\HasleoBackupSuiteService","Start",2
RegWrite,HKLM,REG_DWORD,"Tmp_System\ControlSet001\Services\HasleoBackupSuiteService","ErrorControl",1
RegWrite,HKLM,REG_SZ,"Tmp_System\ControlSet001\Services\HasleoBackupSuiteService","ImagePath","%PEPrograms%\%ProgramFolder%\bin\BackupService.exe"
RegWrite,HKLM,REG_SZ,"Tmp_System\ControlSet001\Services\HasleoBackupSuiteService","DisplayName","Hasleo Backup Suite Service"
RegWrite,HKLM,REG_SZ,"Tmp_System\ControlSet001\Services\HasleoBackupSuiteService","ObjectName","LocalSystem"
RegWrite,HKLM,REG_DWORD,"Tmp_System\ControlSet001\Services\HasleoBackupSuiteService","PreshutdownTimeout",-2147483648

// Virtual Disk Driver
RegWrite,HKLM,REG_DWORD,"Tmp_System\ControlSet001\Services\hasldisk","Type",1
RegWrite,HKLM,REG_DWORD,"Tmp_System\ControlSet001\Services\hasldisk","Start",2
RegWrite,HKLM,REG_DWORD,"Tmp_System\ControlSet001\Services\hasldisk","ErrorControl",0
RegWrite,HKLM,REG_EXPAND_SZ,"Tmp_System\ControlSet001\Services\hasldisk","ImagePath","System32\drivers\hasldisk.sys"
RegWrite,HKLM,REG_SZ,"Tmp_System\ControlSet001\Services\hasldisk","DisplayName","Hasleo Virtual Disk Driver"
RegWrite,HKLM,REG_SZ,"Tmp_System\ControlSet001\Services\hasldisk","Description","Disk Emulation Driver"

// Image Mount Service
RegWrite,HKLM,REG_DWORD,"Tmp_System\ControlSet001\Services\HasleoImageMountService","Type",16
RegWrite,HKLM,REG_DWORD,"Tmp_System\ControlSet001\Services\HasleoImageMountService","Start",2
RegWrite,HKLM,REG_DWORD,"Tmp_System\ControlSet001\Services\HasleoImageMountService","ErrorControl",1
RegWrite,HKLM,REG_EXPAND_SZ,"Tmp_System\ControlSet001\Services\HasleoImageMountService","ImagePath","%PEPrograms%\%ProgramFolder%\bin\ImageMountService.exe"
RegWrite,HKLM,REG_SZ,"Tmp_System\ControlSet001\Services\HasleoImageMountService","DisplayName","Hasleo Backup Suite Image Mount Service"
RegWrite,HKLM,REG_SZ,"Tmp_System\ControlSet001\Services\HasleoImageMountService","ObjectName","LocalSystem"

RegUnloadHives

///////////////////////////////////////////////////////////////////////////////////
// Shortcuts

If,%cb_DesktopShc%,Equal,True,AddShortcut,Desktop,,%txt_ShortcutName%,"%PEPrograms%\%ProgramFolder%\bin\%ProgramExe%"
If,%cb_StartMenuShc%,Equal,True,AddShortcut,StartMenu,%txt_StartMenuFolder%,%txt_ShortcutName%,"%PEPrograms%\%ProgramFolder%\bin\%ProgramExe%"
If,%cb_PinToTaskbar%,Equal,True,PinShortcut,Taskbar,Auto,"%PEPrograms%\%ProgramFolder%\bin\%ProgramExe%"
If,%cb_PinToStartMenu%,Equal,True,PinShortcut,StartMenu,Auto,"%PEPrograms%\%ProgramFolder%\bin\%ProgramExe%"

[#ExtractProgram#]
// ===============================================================================================================================
// Name...........: ExtractProgram
// Description....: Extract program files from script/setup files.
// Syntax.........: Run,%ScriptFile%,ExtractProgram,<DestDir>
// Parameters.....: #1 DestDir - The directory where the files will be extracted.
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: 
// ===============================================================================================================================
[ExtractProgram]
Echo,"Extracting files..."
InnoExtract,"%ProgramsCache%\%ProgramFolder%\%SetupFile%","%TargetPrograms%\%ProgramFolder%\","CDIR={app}\bin\WINPE\Program Files\Hasleo Backup Suite"

If,%SourceArch%,Equal,x64,InnoRename,"%TargetPrograms%\%ProgramFolder%\","*.*",2
Else,InnoRename,"%TargetPrograms%\%ProgramFolder%\","*.*",1

InnoCleanup,"%TargetPrograms%\%ProgramFolder%\","*.*"

[#DownloadProgram#]
// ===============================================================================================================================
// Name...........: DownloadProgram
// Description....: Download the program files from the author's website.
// Syntax.........: Run,%ScriptFile%,DownloadProgram
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: 
// ===============================================================================================================================
[DownloadProgram]
Echo,"Downloading %ScriptTitle%..."
WebGet,%DownloadURL%,"%ProgramsCache%\%ProgramFolder%\%SetupFile%",NOERR
If,Not,#r,Equal,200,Halt,"Download failed: The code returned was [#r]."

[#LaunchProgram#]
// ===============================================================================================================================
// Name...........: LaunchProgram
// Description....: Unpack and launch the program.
// Syntax.........: Run,%ScriptFile%,LaunchProgram
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_LaunchProgram
// ===============================================================================================================================
[LaunchProgram]
Echo,"Launching %ScriptTitle%..."
If,Not,ExistFile,"%ProgramsCache%\%ProgramFolder%\%SetupFile%",Run,%ScriptFile%,DownloadProgram
Run,%ScriptFile%,ExtractProgram,"%ProjectTemp%\%ProgramFolder%"
ShellExecuteEx,Open,"%ProjectTemp%\%ProgramFolder%\%ProgramExe%"

[#ClearDownloadCache#]
// ===============================================================================================================================
// Name...........: ClearDownloadCache
// Description....: Remove all downloaded setup and program files.
// Syntax.........: Run,%ScriptFile%,ClearDownloadCache
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_PurgeCache
// ===============================================================================================================================
[ClearDownloadCache]
DirDeleteEx,"%ProgramsCache%\%ProgramFolder%"

[#SetDefaultOptions#]
// ===============================================================================================================================
// Name...........: SetDefaultOptions
// Description....: Sets script options to default values.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........: 
// Related........: btn_SetDefaultOptions
// ===============================================================================================================================
[SetDefaultOptions]
System,Cursor,Wait
WriteInterface,Value,%ScriptFile%,Interface,cb_RunFromRam,True
WriteInterface,Value,%ScriptFile%,Interface,cb_AlwaysDownload,False
WriteInterface,Value,%ScriptFile%,Interface,cb_DesktopShc,False
WriteInterface,Value,%ScriptFile%,Interface,cb_StartMenuShc,True
WriteInterface,Value,%ScriptFile%,Interface,cb_PinToTaskbar,False
WriteInterface,Value,%ScriptFile%,Interface,cb_PinToStartMenu,False
WriteInterface,Value,%ScriptFile%,Interface,txt_StartMenuFolder,"Backup & Imaging"
WriteInterface,Value,%ScriptFile%,Interface,txt_ShortcutName,"Hasleo Backup Suite"
WriteInterface,Value,%ScriptFile%,Interface,cmb_Language,"English (US)"
System,Cursor,Normal

[#ToggleAdvancedOptions#]
// ===============================================================================================================================
// Name...........: ToggleAdvancedOptions
// Description....: Show/Hide advanced options.
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_AdvancedOptions
// ===============================================================================================================================
[ToggleAdvancedOptions]
Message,"There are no advanced options available for this script."

[#ShowScriptInfo#]
// ===============================================================================================================================
// Name...........: ShowScriptInfo
// Description....: 
// Syntax.........: 
// Parameters.....: 
// Return values..: 
// Author.........: Homes32
// Remarks........:
// Related........: btn_ScriptInfo
// ===============================================================================================================================
[ShowScriptInfo]
Message,"Hasleo Backup Suite Free is a free Windows backup, restore, and cloning software.#$x#$xOn first run this script will download the setup file (~32 MB) and cache it for future builds."

